    "php" : ">=5.6",
    "zf-commons/zfc-user" : "3.*"
  },
      "require-dev" : {
        "phpunit/phpunit" : ">=3.7,<4",
        "squizlabs/php_codesniffer" : "1.4.*"
    },
  "authors" : [ {
    "name" : "Stijnhau",
    "email" : "stijnhau@gmail.com"
  } ],
  "keywords" : [ "zf2", "module", "zendframework" ],
  "autoload" : {
    "psr-0" : {
      "PrivateMessaging" : "src/"
    },
    "classmap" : [ "./Module.php" ]
  },
  "suggest" : {
    "rwoverdijk/assetmanager" : "To use assets(js, css) provided by this module"
  },
  "type" : "library",
  "homepage" : "https://github.com/stijnhau/ZfPrivateMessaging"
